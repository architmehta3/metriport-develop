{
	{{#if code.originalText}}
		"text":"{{{code.originalText._}}}",
	{{/if}}
	"coding": 
    [
		{{>DataType/Coding.hbs code=code}},
		{{#if code.translation.code}}
			{{>DataType/Coding.hbs code=code.translation}},
		{{else}}
			{{#each code.translation}}
				{{>DataType/Coding.hbs code=this}},
			{{/each}}
		{{/if}}
	],
}