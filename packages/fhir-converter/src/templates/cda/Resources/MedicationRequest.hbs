{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "MedicationRequest",
        "id":"{{ID}}",
        "identifier":
  		[
        	{{#each (toArray medicationRequest.id)}}
            	{{>DataType/Identifier.hbs id=this}}
            {{/each}}
        ],
        "intent":"order",
        "status":{{>ValueSet/MedicationRequestStatus.hbs code=medicationRequest.statusCode.code}},
    },
