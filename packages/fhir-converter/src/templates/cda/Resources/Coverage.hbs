{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Coverage",
        "id":"{{ID}}",
        "identifier":
        [
            {{#each (toArray payerEntry.id)}}
            	{{>DataType/Identifier.hbs id=this}},
            {{/each}}
        ], 
        "status":{{>ValueSet/FMStatus.hbs payerEntry.statusCode.code}},
        "relationship":{{>DataType/CodeableConcept.hbs code=payerEntry.code}},
        {{!-- Missing CCDA Payer > FHIR payor --}}
        {{!-- Missing CCDA Covered Party > FHIR beneficiary --}}
        {{!-- Missing CCDA Policy Holder > FHIR policyHolder --}}
        {{!-- Missing CCDA Guarantor > FHIR Participant Extension --}}
    },
    "request":{
        "method":"PUT",
        "url":"Coverage/{{ID}}",
    },
},
